<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <welcome-file-list>
    <welcome-file>index.jsp</welcome-file>
  </welcome-file-list>
  <context-param>
    <param-name>resteasy.servlet.mapping.prefix</param-name>
    <param-value>/api</param-value>
  </context-param>
  <servlet>
    <servlet-name>HelloWorld</servlet-name>
    <servlet-class>aiss.HelloWorldServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>HelloWorld</servlet-name>
    <url-pattern>/helloworld</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>BookSearchController</display-name>
    <servlet-name>bookSearchController</servlet-name>
    <servlet-class>aiss.controller.bookSearchController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>bookSearchController</servlet-name>
    <url-pattern>/bookSearchController</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>BookShowController</display-name>
    <servlet-name>bookShowController</servlet-name>
    <servlet-class>aiss.controller.bookShowController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>bookShowController</servlet-name>
    <url-pattern>/bookShowController</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <servlet-name>RedditOauth2Callback</servlet-name>
    <servlet-class>aiss.controller.oauth.RedditOauth2Callback</servlet-class>
    <init-param>
      <param-name>provider</param-name>
      <param-value>Reddit</param-value>
    </init-param>
    <init-param>
      <param-name>onSuccess</param-name>
      <param-value>redirect:/redditUser</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>RedditOauth2Callback</servlet-name>
    <url-pattern>/oauth2callback/Reddit</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>RedditAuthController</display-name>
    <servlet-name>RedditAuthController</servlet-name>
    <servlet-class>aiss.controller.oauth.RedditAuthController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RedditAuthController</servlet-name>
    <url-pattern>/RedditAuthController</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>RedditLogOut</display-name>
    <servlet-name>RedditLogOut</servlet-name>
    <servlet-class>aiss.controller.RedditLogOut</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RedditLogOut</servlet-name>
    <url-pattern>/redditLogOut</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>RedditSendPost</display-name>
    <servlet-name>RedditSendPost</servlet-name>
    <servlet-class>aiss.controller.RedditSendPost</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RedditSendPost</servlet-name>
    <url-pattern>/redditSendPost</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>MerchController</display-name>
    <servlet-name>MerchController</servlet-name>
    <servlet-class>aiss.controller.MerchController</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>MerchController</servlet-name>
    <url-pattern>/MerchController</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>RedditNewPost</display-name>
    <servlet-name>RedditNewPost</servlet-name>
    <servlet-class>aiss.controller.RedditNewPost</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>RedditNewPost</servlet-name>
    <url-pattern>/redditNewPost</url-pattern>
  </servlet-mapping>
  <servlet>
    <servlet-name>Resteasy</servlet-name>
    <servlet-class>org.jboss.resteasy.plugins.server.servlet.HttpServletDispatcher</servlet-class>
    <init-param>
      <param-name>javax.ws.rs.Application</param-name>
      <param-value>aiss.api.BookAssistantApplication</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>Resteasy</servlet-name>
    <url-pattern>/api/*</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <servlet-name>GoogleBooksAuthController</servlet-name>
    <servlet-class>aiss.controller.oauth.GenericAuthController</servlet-class>
    <init-param>
      <param-name>provider</param-name>
      <param-value>GoogleBooks</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>GoogleBooksAuthController</servlet-name>
    <url-pattern>/AuthController/GoogleBooks</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <servlet-name>GoogleBooksOAuth2Callback</servlet-name>
    <servlet-class>aiss.controller.oauth.OAuth2Callback</servlet-class>
    <init-param>
      <param-name>provider</param-name>
      <param-value>GoogleBooks</param-value>
    </init-param>
    <init-param>
      <param-name>onSuccess</param-name>
      <param-value>redirect:/bookShowController</param-value>
    </init-param>
  </servlet>
  <servlet-mapping>
    <servlet-name>GoogleBooksOAuth2Callback</servlet-name>
    <url-pattern>/oauth2callback/GoogleBooks</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>BookShelfControlller</display-name>
    <servlet-name>BookShelfControlller</servlet-name>
    <servlet-class>aiss.controller.BookShelfControlller</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>BookShelfControlller</servlet-name>
    <url-pattern>/bookShelfControlller</url-pattern>
  </servlet-mapping>
</web-app>